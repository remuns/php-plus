{
  "name": "remuns/phpplus-core",
  "description": "PHP-Plus is a standard core library containing auxiliary functionality for working with PHP.",
  "license": "MIT",
  "keywords": [
    "PHP-Plus"
  ],
  "authors": [
    {
      "name": "Richard Munson",
      "email": "remuns19@gmail.com",
      "homepage": "https://github.com/remuns/",
      "role": "Coffee Maker :)"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "5.2.*",
    "phpunit/phpunit": "9.3"
  },
  "require-dev": {
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*"
  },
  "autoload": {
    "psr-4": {
      "PhpPlus\\Core\\": "src",
      "PhpPlus\\Core\\Library\\": "src/library"
    },
    "files": [
      "src/top-level.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PhpPlus\\Core\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
